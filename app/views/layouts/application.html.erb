<!DOCTYPE html>
<html ng-app="entrytest">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>EntryTest</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<ul class="nav nav-tabs">
	<li class="active">
		<a href="<%= root_path %>">Home</a>
	</li>
	<li class="dropdown">
		<a class="dropdown-toggle" data-toggle="dropdown" href="#">questions <span class="caret"></span></a>
		<ul class="dropdown-menu">
			<li><a href="<%= questions_path %>">questions</a></li>
			<li><a href="<%= new_question_path %>">New question</a></li>
			<li class="divider"></li>
			<li><a href="<%= answers_path %>">answers</a></li>
			<li><a href="<%= labels_path %>">labels</a></li>
		</ul>
	</li>
	<li class="dropdown">
		<a class="dropdown-toggle" data-toggle="dropdown" href="#">tests <span class="caret"></span></a>
		<ul class="dropdown-menu">
			<li><a href="<%= tests_path %>">tests</a></li>
			<li><a href="<%= attempts_path %>">attempts</a></li>
			<li class="divider"></li>
			<li><a href="<%= timetables_path %>">timetables</a></li>
		</ul>
	</li>
	<li><a href="<%= users_path %>">users</a></li>
	<li><a href="#" class="pull-right">Log in</a></li>
</ul>
<div class="container">
<div class="row ">
	<% flash.each do |name, msg| %>
  <div class="alert alert-<%= name %>">
    <a class="close" data-dismiss="alert">&#215;</a>
    <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
  </div>
<% end %>
</div>
	<div class="row">
		<%= yield %>
	</div>
</div>
</body>
</html>
